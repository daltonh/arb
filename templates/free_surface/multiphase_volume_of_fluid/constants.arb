MARKDOWN
Equivalent of constants for the vof templates, but here setting all constants
END_MARKDOWN

{{
  my $return_string = '';
  for my $species (string_eval('<<multiphase_vof_species_list>>','list')) {
    $return_string .= "INCLUDE_TEMPLATE 'free_surface/volume_of_fluid/constants' R '<sigma>' W '<sigma_$species>' R '<mu>' W '<mu_$species>' R '<S>' W '<S_$species>' R '<Ca>' W '<Ca_$species>' R '<We>' W '<We_$species>'\n";
  }
  return $return_string;
}}
