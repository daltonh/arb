total_stress.arb:#REPLACEMENTS D "<<stress_method>>" W "version1" # the gamma used in each phase is specific to that phase, but stress is not specific to phase (see conservation template)
total_stress.arb:REPLACEMENTS D "<<stress_method>>" W "version2" # the gamma used in each phase is a function of all velocities
total_stress.arb:#REPLACEMENTS D "<<stress_method>>" W "version3" # the gamma used in each phase is specific to that phase, but stress is specific to phase (see conservation template)
total_stress.arb:IF {{ return (string_test('<<stress_method>>','version1') || string_test('<<stress_method>>','version3')) }}
total_stress.arb:  IF {{ return (string_test('<<stress_method>>','version2') || string_test('<<stress_method>>','jackson98')) }}
version1_setup.arb:GLOBAL_REPLACEMENTS R "<<stress_method>>" W "version1"
